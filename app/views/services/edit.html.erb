					<%= form_for :service, url: service_path(@service), method: :patch, :html => { :id => "service_new", :class => "form-horizontal form-label-left" } do |f| %>
  

  <div class="form-group">
      <%= f.label :title, 'Title <span>*</span>'.html_safe ,  :class=> 'control-label col-md-3 col-sm-3 col-xs-12' %>
      <div class="col-md-6 col-sm-6 col-xs-12">
      <%= f.text_field :title, :id => "title", :required=> "required", :rows=> "25", :class=>"form-control col-md-7 col-xs-12 parsley-error"%>
    </div>
  </div>


  <div class="form-group">
      <%= f.label :text, 'Description <span>*</span>'.html_safe ,  :class=> 'control-label col-md-3 col-sm-3 col-xs-12' %>
      <div class="col-md-6 col-sm-6 col-xs-12">
      <%= f.text_area :text, :id => "desc", :required=> "required", :class=>"form-control col-md-7 col-xs-12 parsley-error"%>
    </div>
  </div>


  <div class="form-group">
      <%= f.label :fa_icon, 'Font Awesome Icon <span>*</span>'.html_safe ,  :class=> 'control-label col-md-3 col-sm-3 col-xs-12' %>
      <div class="col-md-6 col-sm-6 col-xs-12">
      <%= f.text_field :fa_icon, :id => "title", :required=> "required", :class=>"form-control col-md-7 col-xs-12 parsley-error"%>
    </div>
  </div>

  <div class="form-group">
      <%= f.label :publihsed, 'Published '.html_safe , :class => 'control-label col-md-3 col-sm-3 col-xs-12' %>
      <div class="col-md-6 col-sm-6 col-xs-12">
      <%= f.check_box :enabled , :checked => 'checked' %>
    </div>
  </div>

  <div class="form-group">
      <%= f.label :weight, 'Weight '.html_safe ,  :class=> 'control-label col-md-3 col-sm-3 col-xs-12' %>
      <div class="col-md-3 col-sm-6 col-xs-12">
       <%= f.text_field :weight, :id => "weight", :required=> "required",  :class=>"form-control col-md-7 col-xs-12 parsley-error"%>
    </div>
  </div>
 

                      <div class="ln_solid"></div>
                      <div class="form-group">
                        <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                        <%= f.submit:submit, :class => 'btn btn-success' %>
                        <%= button_tag "Reset", :type => 'reset', :class => 'btn btn-primary' %>
                        </div>
                      </div>

                    <% end %>
                  </div>
                </div>
